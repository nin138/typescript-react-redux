    "sinon": "8.1.1",
    "style-loader": "1.1.3",
    "stylelint": "13.3.0",
    "stylelint-config-prettier": "8.0.1",
    "stylelint-config-standard": "19.0.0",
    "terser-webpack-plugin": "2.3.5",
    "ts-jest": "25.3.1",
    "ts-loader": "6.2.2",
    "tslint": "5.20.1",
    "tslint-config-prettier": "1.18.0",
    "tslint-plugin-prettier": "2.3.0",
    "tslint-react": "4.1.0",
