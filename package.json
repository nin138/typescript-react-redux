{
  "name": "react-ts-redux-saga-example",
  "version": "2.0.0",
  "license": "MIT",
  "scripts": {
    "start": "yarn tcm-watch & webpack-dev-server --open --mode development --inline --hot --localhost --config build/webpack.config.dev.js",
    "prebuild": "tcm src/css",
    "build": "webpack --mode production --config build/webpack.config.prod.js",
    "tcm-watch": "tcm -w src/css",
    "test": "jest --coverage",
    "fmt": "yarn tslint --fix & yarn stylelint --fix & yarn prettier",
    "prettier": "prettier --write './**/*.{ts,tsx,css,yml,js}'",
    "lint": "yarn tslint && yarn stylelint",
    "tslint": "tslint -p tsconfig.json 'src/ts/*.{ts,tsx}'",
    "stylelint": "stylelint './src/**/*.css'"
  },
  "dependencies": {
    "@babel/runtime": "7.7.4",
    "loaders.css": "0.1.2",
    "react": "16.12.0",
    "react-dom": "16.12.0",
    "react-redux": "7.1.3",
    "react-router-dom": "5.1.2",
    "redux": "4.0.4",
    "redux-logger": "3.0.6",
    "redux-saga": "1.1.3"
  },
  "devDependencies": {
    "@babel/core": "7.7.4",
    "@babel/plugin-transform-runtime": "7.7.4",
    "@babel/polyfill": "7.7.0",
    "@babel/preset-env": "7.7.4",
    "@babel/preset-react": "7.7.4",
    "@redux-saga/testing-utils": "1.1.3",
    "@types/enzyme": "3.10.3",
    "@types/jest": "24.0.23",
    "@types/react": "16.9.15",
    "@types/react-dom": "16.9.4",
    "@types/react-redux": "7.1.5",
    "@types/react-router-dom": "4.3.5",
    "@types/react-test-renderer": "16.9.1",
    "@types/redux-logger": "3.0.7",
    "@types/sinon": "7.5.1",
    "babel-loader": "8.0.6",
    "babel-plugin-transform-react-jsx-source": "6.22.0",
    "clean-webpack-plugin": "3.0.0",
    "copy-webpack-plugin": "5.0.5",
    "css-loader": "3.2.1",
    "enzyme": "3.10.0",
    "html-loader": "0.5.5",
    "html-webpack-plugin": "3.2.0",
    "husky": "2.7.0",
    "identity-obj-proxy": "3.0.0",
    "jest": "24.9.0",
    "lint-staged": "8.2.1",
    "mini-css-extract-plugin": "0.8.0",
    "optimize-css-assets-webpack-plugin": "5.0.3",
    "postcss-import": "12.0.1",
    "postcss-loader": "3.0.0",
    "postcss-preset-env": "6.7.0",
    "prettier": "1.19.1",
    "react-test-renderer": "16.12.0",
    "redux-saga-test-plan": "4.0.0-rc.3",
    "sinon": "7.5.0",
    "style-loader": "1.0.1",
    "stylelint": "10.1.0",
    "stylelint-config-prettier": "8.0.0",
    "stylelint-config-standard": "18.3.0",
    "ts-jest": "24.2.0",
    "ts-loader": "6.2.1",
    "tslint": "5.20.1",
    "tslint-config-prettier": "1.18.0",
    "tslint-plugin-prettier": "2.0.1",
    "tslint-react": "4.1.0",
    "typed-css-modules": "0.6.3",
    "typescript": "3.7.3",
    "uglifyjs-webpack-plugin": "2.2.0",
    "webpack": "4.41.2",
    "webpack-bundle-analyzer": "3.6.0",
    "webpack-cli": "3.3.10",
    "webpack-dev-server": "3.9.0",
    "webpack-merge": "4.2.2"
  },
  "lint-staged": {
    "linters": {
      "*.{ts,tsx,css,js,json,yml}": [
        "yarn run fmt",
        "git add"
      ]
    },
    "ignore": [
      "dist",
      "coverage"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  }
}
