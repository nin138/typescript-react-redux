{
  "name": "react-ts-redux-saga-example",
  "version": "2.0.0",
  "license": "MIT",
  "scripts": {
    "start": "yarn tcm-watch & webpack-dev-server --open --mode development --inline --hot --localhost --config build/webpack.config.dev.js",
    "prebuild": "tcm src/css",
    "build": "webpack --mode production --config build/webpack.config.prod.js",
    "tcm-watch": "tcm -w src/css",
    "test": "jest --coverage",
    "fmt": "yarn tslint --fix & yarn stylelint --fix & yarn prettier",
    "prettier": "prettier --write './**/*.{ts,tsx,css,yml,js}'",
    "lint": "yarn tslint && yarn stylelint",
    "tslint": "tslint -p tsconfig.json 'src/ts/*.{ts,tsx}'",
    "stylelint": "stylelint './src/**/*.css'"
  },
  "dependencies": {
    "@babel/runtime": "7.4.5",
    "loaders.css": "^0.1.2",
    "react": "16.8.6",
    "react-dom": "16.8.6",
    "react-redux": "7.0.3",
    "react-router-dom": "5.0.0",
    "redux": "4.0.1",
    "redux-logger": "3.0.6",
    "redux-saga": "1.0.2"
  },
  "devDependencies": {
    "@babel/core": "7.4.5",
    "@babel/plugin-transform-runtime": "7.4.4",
    "@babel/polyfill": "7.4.4",
    "@babel/preset-env": "7.4.5",
    "@babel/preset-react": "7.0.0",
    "@redux-saga/testing-utils": "^1.0.2",
    "@types/enzyme": "^3.9.3",
    "@types/jest": "24.0.13",
    "@types/react": "16.8.19",
    "@types/react-dom": "16.8.4",
    "@types/react-redux": "7.0.9",
    "@types/react-router-dom": "4.3.3",
    "@types/react-test-renderer": "16.8.1",
    "@types/redux-logger": "3.0.7",
    "@types/sinon": "^7.0.12",
    "babel-loader": "8.0.6",
    "babel-plugin-transform-react-jsx-source": "6.22.0",
    "clean-webpack-plugin": "2.0.2",
    "copy-webpack-plugin": "5.0.3",
    "css-loader": "2.1.1",
    "enzyme": "^3.10.0",
    "html-loader": "0.5.5",
    "html-webpack-plugin": "3.2.0",
    "husky": "2.3.0",
    "identity-obj-proxy": "3.0.0",
    "jest": "24.8.0",
    "lint-staged": "8.1.7",
    "mini-css-extract-plugin": "0.7.0",
    "optimize-css-assets-webpack-plugin": "5.0.1",
    "postcss-import": "12.0.1",
    "postcss-loader": "3.0.0",
    "postcss-preset-env": "6.6.0",
    "prettier": "1.17.1",
    "react-test-renderer": "16.8.6",
    "redux-saga-test-plan": "^4.0.0-beta.3",
    "sinon": "^7.3.2",
    "style-loader": "0.23.1",
    "stylelint": "10.0.1",
    "stylelint-config-prettier": "5.2.0",
    "stylelint-config-standard": "18.3.0",
    "ts-jest": "24.0.2",
    "ts-loader": "6.0.2",
    "tslint": "5.17.0",
    "tslint-config-prettier": "1.18.0",
    "tslint-plugin-prettier": "2.0.1",
    "tslint-react": "4.0.0",
    "typed-css-modules": "0.4.2",
    "typescript": "3.5.1",
    "uglifyjs-webpack-plugin": "2.1.3",
    "webpack": "4.32.2",
    "webpack-bundle-analyzer": "3.3.2",
    "webpack-cli": "3.3.2",
    "webpack-dev-server": "3.5.1",
    "webpack-merge": "4.2.1"
  },
  "lint-staged": {
    "linters": {
      "*.{ts,tsx,css,js,json,yml}": [
        "yarn run fmt",
        "git add"
      ]
    },
    "ignore": [
      "dist",
      "coverage"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  }
}
