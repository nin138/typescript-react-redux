    "babel-loader": "8.1.0",
    "babel-plugin-transform-react-jsx-source": "6.22.0",
    "clean-webpack-plugin": "3.0.0",
    "copy-webpack-plugin": "5.0.5",
    "css-loader": "3.4.2",
    "enzyme": "3.11.0",
    "html-loader": "0.5.5",
    "html-webpack-plugin": "3.2.0",
    "husky": "4.2.3",
    "identity-obj-proxy": "3.0.0",
